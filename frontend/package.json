{
  "name": "printsync3d-ui",
  "version": "1.0.0",
  "author": "Alan BuriÄ‡",
  "description": "Web application for centralized management of 3D printers via a Vue.js website",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p \"build-only {@}\" --",
    "build-gh-pages": "vite build --base=/printsync3d/",
    "start:vite": "vite preview",
    "start:vite-expose": "vite preview --host",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@tailwindcss/vite": "^4.0.9",
    "@tsconfig/node24": "^24.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.29",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint-plugin-vue": "^10.1.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}
